<launch>
  <include file="$(find ontologenius)/launch/ontologenius.launch">
    <arg name="intern_file" default="none"/>
    <arg name="display" default="false"/>
  </include>
  <arg name='TESTDURATION' value='60' />
  <test pkg="ontologenius" type="onto_api_relations_test" test-name="api_relations_test" time-limit="$(arg TESTDURATION)" retry="0" />
</launch>
